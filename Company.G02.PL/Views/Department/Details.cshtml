@model Department; 
@{
    ViewData["Title"] = "Details";
}

<h1>Details Page</h1>


<br />
<br />

<div>
    <div>
        <div>
            <label asp-for="Code" class="form-label"></label>
            <input asp-for="Code" class="form-control" readonly/>
        </div>

        <div>
            <label asp-for="Name" class="form-label mt-3"></label>
            <input asp-for="Name" class="form-control" readonly/>
        </div>
        <div>

            <label asp-for="CreateAt" class="form-label mt-3"></label>
            <input asp-for="CreateAt" class="form-control" readonly/>
        </div>
    </div>

    <div class="m-4 ">

        <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-warning mt-3">Edit</a>
        <a asp-action="Index" class="btn btn-light mt-3">Back to List</a>

    </div>
</div>

